<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #faf8ef;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.loseController">
   <children>
      <Label layoutX="50.0" layoutY="14.0" prefHeight="24.0" prefWidth="100.0" text="Game Over" textFill="#84776e">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </Label>
      <Button fx:id="tryAgain" layoutX="61.0" layoutY="50.0" mnemonicParsing="false" onAction="#tryAgain" style="-fx-background-color: #84776e;" text="Try again" textFill="#faf8ef">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
