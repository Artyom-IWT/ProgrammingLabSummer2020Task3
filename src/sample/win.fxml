<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #faf8ef;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Label alignment="CENTER" layoutX="21.0" layoutY="14.0" prefHeight="26.0" prefWidth="158.0" text="You Win!" textAlignment="CENTER" textFill="#84776e">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Button fx:id="playAgain" alignment="CENTER" contentDisplay="CENTER" layoutX="58.0" layoutY="50.0" mnemonicParsing="false" onAction="#playAgain" style="-fx-background-color: #84776e;" text="Play again" textFill="#faf8ef">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
